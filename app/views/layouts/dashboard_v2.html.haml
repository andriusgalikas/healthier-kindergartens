!!!
%html
  %head
    %meta{:content => "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}/
    %title HealthChildcare
    = stylesheet_link_tag  'dashboard', media: 'all', 'data-turbolinks-track' => true
    = stylesheet_link_tag  'introjs', media: 'all', 'data-turbolinks-track' => true
    %link{:href => "https://fonts.googleapis.com/css?family=Open+Sans:400,300,300italic,400italic,600,600italic,700,700italic,800,800italic", :rel => "stylesheet", :type => "text/css"}
    = javascript_include_tag 'application', 'data-turbolinks-track' => true
    %script{src: "https://www.gstatic.com/charts/loader.js"}
    = tag :meta, name: "stripe-key", content: Rails.application.secrets.stripe_public_key
    = csrf_meta_tags
    %meta{:content => "width=device-width, initial-scale=1", :name => "viewport"}
    :javascript
      window._trans = #{yield_js_translations.to_json.html_safe}
    :javascript
      $crisp=[];
      CRISP_WEBSITE_ID="2a0e587f-839b-4b8c-9ef3-3881e449188e";
      (function(){
        d=document;
        s=d.createElement("script");
        s.src="https://client.crisp.im/l.js";
        s.async=1;
        d.getElementsByTagName("head")[0].appendChild(s);
      })();
  %body
    = render partial: 'shared/headers/dashboard_v2'
    #dashboard.main
      = yield
    = render partial: 'shared/footer_dashboard'
